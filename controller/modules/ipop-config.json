{
  "CFx": {
    "Model": "GroupVPN"
  },
  "Logger": {
    "LogLevel": "ERROR",
    "LogOption": "Console",
    "BackupLogFileCount": 5,
    "LogFileName": "ctr.log",
    "LogFileSize": 10000
  },
  "Tincan": {
    "Log": {
      "Level": "ERROR",
      "Device": "Console",
      "Directory": "./logs/",
      "Filename": "tincan_log",
      "MaxArchives": 10,
      "MaxFileSize": 1048576,
      "ConsoleLevel": "NONE"
    },
    "Vnets": [
      {
        "IP4": "172.31.0.1",
        "IP4Prefix": 16,
        "XMPPModuleName": "XmppClient",
        "TapName": "ipop_tap0",
        "Description": "Ethernet Device",
        "IgnoredNetInterfaces": [ "ipop_tap0", "ipop_tap1", "Bluetooth Network Connection", "VMware Network Adapter VMnet1", "VMware Network Adapter VMnet2" ],
        "L2TunnellingEnabled": 1
      }
    ],
    "Stun": [ "stun.l.google.com:19302" ],
    "Turn": [
      {
        "Address": "*.*.*.*:19302",
        "User": "***",
        "Password": "***"
      }
    ]
  },
  "XmppClient": {
    "Enabled": true,
    "Username": "",
    "Password": "",
    "AddressHost": "",
    "Port": "5222",
    "TapName": "ipop_tap0",
    "AuthenticationMethod": "password",
    "AcceptUntrustedServer": true,
    "TimerInterval": 15,
    "dependencies": [ "Logger" ]
  },
  "BaseTopologyManager": {
    "NumberOfSuccessors": 2,
    "NumberOfChords": 3,
    "NumberOfOnDemand": 8,
    "NumberOfInbound": 20,
    "InitialLinkTTL": 120,
    "LinkPulse": 180,
    "OnDemandLinkTTL": 60,
    "TimerInterval": 1,
    "TopologyRefreshInterval": 15,
    "NumberOfPingsToPeer": 5,
    "PeerPingInterval": 300,
    "MaxConnRetry": 5,
    "dependencies": [ "Logger" ]
  },
  "TincanDispatcher": {
    "dependencies": [ "Logger" ]
  },
  "TincanListener": {
    "SocketReadWaitTime": 15,
    "dependencies": [ "Logger", "TincanDispatcher" ]
  },
  "TincanSender": {
    "dependencies": [ "Logger" ]
  },
  "OverlayVisualizer": {
    "Enabled": false,
    "WebServiceAddress": "*:8080/insertdata",
    "TopologyDataQueryInterval": 5,
    "WebServiceDataPostInterval": 10,
    "TimerInterval": 5,
    "NodeName": "*",
    "dependencies": [ "Logger" ]
  },
  "BroadCastController": {
    "Enabled": true,
    "dependencies": [ "Logger" ]
  },
  "BroadCastForwarder": {
    "Enabled": true,
    "dependencies": [ "Logger" ]
  },
  "Multicast": {
    "Enabled": true,
    "OnDemandThreshold": 15,
    "dependencies": [ "Logger" ]
  },
  "ConnectionManager": {
    "InitialLinkTTL": 120,
    "ChordLinkTTL": 180,
    "OndemandLinkRateThreshold": 128,
    "dependencies": [ "Logger" ]
  }
}
